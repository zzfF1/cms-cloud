<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.notice.mapper.SysNotificationMergeConfigMapper">

    <select id="selectByTemplateCode" resultType="com.sinosoft.notice.domain.SysNotificationMergeConfig">
        SELECT *
        FROM sys_notification_merge_config
        WHERE template_code = #{templateCode}
        LIMIT 1
    </select>
</mapper>
