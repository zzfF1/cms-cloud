<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sinosoft.notice.mapper.SysNotificationSettingMapper">

    <select id="selectByUserId" resultType="com.sinosoft.notice.domain.SysNotificationSetting">
        SELECT *
        FROM sys_notification_setting
        WHERE user_id = #{userId}
        LIMIT 1
    </select>
</mapper>
